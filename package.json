    "dotenv": "^16.0.2",
    "inquirer": "^9.1.3",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "@sapphire/snowflake": "^3.2.2",
    "discord.js": "^14.3.0",
    "mongoose": "^6.6.5"
  }
}
