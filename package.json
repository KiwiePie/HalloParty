{
  "name": "halloparty",
  "version": "1.0.0",
  "description": "Hallo Party Discord bot",
  "engines": {
    "node": "18.x"
  },
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --no-warnings .",
